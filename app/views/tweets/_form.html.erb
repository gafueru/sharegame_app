<%= form_with model: @tweet, local: true do |f|%>
  <div class="field">
    <%= f.label :title, "タイトル" %><br />
    <%= f.text_field :title %>
  </div>

  <div class="field">
    <%= f.label :text, "自分のここを頑張ったところ" %><br />
    <%= f.text_area :text %>
  </div>

  <div class="field">
    <%= f.label :image, "画像" %><br />
    <%= f.text_field :image %>
  </div>

  <div class="actions">
    <%= f.submit "保存する", class: :form__btn  %>
  </div>
<% end %>